{
  "name": "SomBrowser",
  "version": "0.7.0",
  "description": "Minimal and modern react+electron+webpack boilerplate",
  "author": "Phillip Barbiero",
  "homepage": "https://github.com/pbarbiero/basic-electron-react-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/pbarbiero/basic-electron-react-boilerplate.git"
  },
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prod": "webpack --config webpack.build.config.js && electron --noDevServer .",
    "dev": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js",
    "build": "webpack --config webpack.build.config.js",
    "package": "webpack --config webpack.build.config.js",
    "postpackage": "node_modules/.bin/electron-packager ./ --out=./builds"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.11",
    "devtron": "^1.4.0",
    "electron": "^1.8.8",
    "electron-packager": "^13.0.1",
    "electron-react-devtools": "^0.5.3",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^2.28.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "style-loader": "^0.19.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  },
  "dependencies": {
    "ajv": "^6.6.2",
    "electron-fetch": "^1.3.0",
    "mathjs": "^5.2.3",
    "meyda": "^4.1.3",
    "react-dnd": "^5.0.0",
    "react-dnd-html5-backend": "^5.0.1",
    "react-dnd-mouse-backend": "^1.0.0-rc.1",
    "react-dnd-touch-backend": "^0.5.2",
    "react-hot-loader": "^4.3.12",
    "reactstrap": "^6.5.0",
    "roboto-fontface": "^0.10.0"
  }
}
